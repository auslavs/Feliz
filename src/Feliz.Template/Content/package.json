{
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "transpile": "dotnet fable src -e fs.jsx --verbose",
    "start": "npm run transpile -- --watch --runFast vite",
    "build": "npm run transpile && vite build",
    "clean": "dotnet fable clean src --yes",
    "test": "npm run clean && npm run transpile -- --watch --run vitest"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "jsdom": "^26.1.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.2"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  }
}
